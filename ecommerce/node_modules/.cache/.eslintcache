 async() => {\n    dispatch(categoryProductsNew(categoryID, -1));\n  };\n  const oldProducts = async() => {\n    dispatch(categoryProductsOld(categoryID, 1));\n  };\n\n  const token = useSelector((state) => state.auth.token);\n  const browserHistory = useHistory();\n  const AddProduct=async(productID)=>{\n    // console.log(productID);\n    if (!token) {\n      browserHistory.push(\"/login\");\n    }\n    dispatch(AddToCart(productID, props.history));\n  }\n  const renderProducts = products\n    .slice(pageVisted, pageVisted + productsPerPage)\n    .map((product) => {\n      var str = product.photo;\n      var url = str.replace(\"/home/sushil/2021/E-Commerce/api/uploads/\", \"\");\n      // console.log(url);\n      var source = \"http://localhost:4000/uploads/\" + url;\n      return (\n        <Fragment key={product._id}>\n          <div className=\"lg:w-1/4 md:w-1/2 p-4 w-full\">\n            <Link\n              to={`/product/${product._id}`}\n              className=\"block relative h-48 rounded overflow-hidden\"\n            >\n              <img\n                alt=\"ecommerce\"\n                src={source}\n                className=\"object-cover object-center w-full h-full block\"\n              />\n            </Link>\n            <div className=\"mt-4\">\n              <h3 className=\"text-gray-500 text-xs tracking-widest title-font mb-1\">\n                {product.categoryID}\n              </h3>\n              <h2 className=\"text-gray-900 title-font text-lg font-medium\">\n                {product.title}\n              </h2>\n              <p className=\"mt-1\">Nrs.{product.price}</p>\n            </div>\n            <div\n              style={{\n                display: \"flex\",\n                justifyContent: \"center\",\n                alignItems: \"center\",\n              }}\n            >\n              <button\n                type=\"button\"\n                onClick={() => AddProduct(product._id)}\n                className=\"btn btn-danger\"\n                style={{ width: \"60%\" }}\n              >\n                <i class=\"fas fa-2x fa-cart-plus\" style={{ color: \"red\" }}></i>\n                &nbsp;&nbsp;Add To cart\n              </button>\n              &nbsp;\n              <button\n                type=\"button\"\n                className=\"btn btn-primary\"\n                style={{ width: \"40%\" }}\n              >\n                <i class=\"fas fa-2x fa-heart\" style={{ color: \"orange\" }}></i>\n                &nbsp;&nbsp;Wishlist\n              </button>\n            </div>\n          </div>\n        </Fragment>\n      );\n    });\n\n  const [formData, setFormData] = useState({\n    search: \"\",\n  });\n\n  const onCategorySearchProduct = async (e) =>\n    setFormData({ ...formData, [e.target.name]: e.target.value });\n\n  const { search } = formData;\n  // console.log(search)\n  useEffect(() => {\n    dispatch(categoryProductSearch(categoryID, search));\n  }, [search, categoryID, dispatch]);\n\n  const refresh = () => {\n    // it re-renders the component\n    window.location.reload();\n  };\n\n  return loading && !products.length > 0 ? (\n    <Spinner />\n  ) : (\n    <Fragment>\n      <div className=\"product-container\">\n        <br />\n        <div className=\"product-header container px-5\">\n          <div>\n            <ul className=\"navbar-nav mb-2 mb-lg-0 ml-auto ml-auto\">\n              <li className=\"nav-item dropdown\">\n                <a\n                  className=\"nav-link active dropdown-toggle\"\n                  href=\"#\"\n                  id=\"navbarDropdown\"\n                  role=\"button\"\n                  data-bs-toggle=\"dropdown\"\n                  aria-expanded=\"false\"\n                >\n                  <i\n                    className=\"fab fa-2x fa-accessible-icon\"\n                    style={{ color: \"red\" }}\n                  ></i>\n                  &nbsp;Filter Products\n                </a>\n                {/* <br /> */}\n                <ul className=\"dropdown-menu\" aria-labelledby=\"navbarDropdown\">\n                  <li className=\"active\">\n                    <a onClick={cheapProducts} className=\"dropdown-item\">\n                      Cheapest\n                    </a>\n                  </li>\n                  <li className=\"active\">\n                    <a onClick={expensiveProducts} className=\"dropdown-item\">\n                      Expensive\n                    </a>\n                  </li>\n                  <li className=\"active\">\n                    <a onClick={newProducts} className=\"dropdown-item\">\n                      Newest first\n                    </a>\n                  </li>\n                  <li className=\"active\">\n                    <a onClick={oldProducts} className=\"dropdown-item\">\n                      Oldest first\n                    </a>\n                  </li>\n                </ul>\n              </li>\n            </ul>\n          </div>\n          <form className=\"d-flex\">\n            <input\n              className=\"form-control me-2\"\n              type=\"search\"\n              name=\"search\"\n              value={search}\n              placeholder=\"Search product\"\n              aria-label=\"Search\"\n              onChange={(e) => onCategorySearchProduct(e)}\n            />\n            {/* <button className=\"btn btn-outline-success\" style={{borderRadius :'0px'}} type=\"submit\">Search</button> */}\n            {/* <button> */}\n            <i className=\"fas fa-3x fa-search\"></i>\n            {/* </button> */}\n          </form>\n        </div>\n        <section class=\"text-gray-600 body-font\">\n          <div class=\"container px-5 py-24 mx-auto\">\n            <div class=\"flex flex-wrap -m-4\">\n              {products.length > 0 ? (\n                renderProducts\n              ) : (\n                <>\n                  <div class=\"no-product\">\n                    <section className=\"flex items-center h-full p-16 bg-coolGray-50 text-coolGray-800\">\n                      <div className=\"container flex flex-col items-center justify-center px-5 mx-auto my-8\">\n                        <div className=\"max-w-md text-center\">\n                          <h2 className=\"mb-8 font-extrabold text-9xl text-coolGray-400\">\n                            <span className=\"sr-only\">Error </span>404\n                          </h2>\n                          <p className=\"text-2xl font-semibold md:text-3xl text-coolGray-600\">\n                            Sorry, we couldn't find this page.\n                          </p>\n                          <p className=\"mt-4 mb-8\">\n                            But dont worry, you can find plenty of other things\n                            on our products page\n                          </p>\n                          <button\n                            onClick={refresh}\n                            className=\"px-8 py-3 font-semibold rounded bg-violet-600 text-coolGray-50\"\n                          >\n                            Reload\n                          </button>\n                        </div>\n                      </div>\n                    </section>\n                  </div>\n                </>\n              )}\n            </div>\n            <br />\n            <br />\n            <ReactPaginate\n              containerClassName={\"paginationBttns\"}\n              previousLinkClassName={\"previousBttns\"}\n              nextLinkClassName={\"nextBttns\"}\n              disabledClassName={\"paginationDisabled\"}\n              activeClassName={\"paginationActive\"}\n              previousLabel={\"Previous\"}\n              nextLabel={\"Next\"}\n              pageCount={pageCount}\n              onPageChange={changePage}\n              breakLabel={\"...\"}\n              marginPagesDisplayed={2}\n              pageRangeDisplayed={5}\n            />\n          </div>\n        </section>\n      </div>\n    </Fragment>\n  );\n};\n\nexport default withRouter(Category);\n","/home/sushil/2021/E-Commerce/ecommerce/src/components/auth/SignUp.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/components/orders/Order.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/components/orders/OrderDetails.js",["196"],"import React, { Fragment, useEffect } from \"react\";\nimport {\n  Row,\n  Container,\n  Alert,\n  Col,\n  ListGroup,\n  Spinner,\n  Button,\n} from \"react-bootstrap\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useHistory, withRouter } from \"react-router-dom\";\nimport { orderDetails, cancelOrder, placeOrderByID } from \"../../actions/order\";\n\nconst OrderDetails = (props) => {\n  const token = useSelector((state) => state.auth.token);\n  const dispatch = useDispatch();\n  const browserHistory = useHistory();\n  const orderID = props.match.params.orderID;\n\n  useEffect(() => {\n    if (!token) {\n      browserHistory.push(\"/login\");\n    }\n    dispatch(orderDetails(orderID));\n    // dispatch(getMyOrders())\n  }, [token, browserHistory, dispatch, orderID]);\n  const { order, loading } = useSelector((state) => state.order);\n const CancelOrder = async (orderID) => {\n    // console.log(productID);\n    dispatch(cancelOrder(orderID, props.history));\n  };\n  const PlaceOrder = async (orderID) => {\n    // console.log(productID);\n    dispatch(placeOrderByID(orderID, props.history));\n  };\n\n\n  if (Object.keys(order).length !== 0 && !loading) {\n    const { products } = order;\n    const renderItems = products.map((product) => {\n      const perProductTotal = product.product.price * product.quantity;\n      var str = product.product.photo;\n      var url = str.replace(\"/home/sushil/2021/E-Commerce/api/uploads/\", \"\");\n      // console.log(url);\n      var source = \"http://localhost:4000/uploads/\" + url;\n\n      return (\n        <Fragment>\n          <div className=\"row border-top border-bottom\">\n            <div className=\"row main align-items-center\">\n              <div className=\"col-2\">\n                <img className=\"img-fluid\" src={source} alt=\"\" />\n              </div>\n              <div className=\"col\">\n                <div className=\"row text-muted\">\n                  {product.product.categoryID.name}\n                </div>\n                <div className=\"row\">{product.product.title}</div>\n              </div>\n              <div className=\"col\">\n                {\" \"}\n                <Fragment>\n                  <a href=\"#\" className=\"border\">\n                    {product.quantity}\n                  </a>\n                  &nbsp;&nbsp;\n                </Fragment>\n              </div>\n              <div className=\"col\">\n                NRS.&nbsp;{product.product.price}\n                <span className=\"close\"></span>\n              </div>\n              <div className=\"col\">\n                Total.&nbsp;{perProductTotal}\n                <span className=\"close\"></span>\n              </div>\n            </div>\n          </div>\n        </Fragment>\n      );\n    });\n    return (\n      <Container>\n        <br />\n        <Row>\n          <Col sm={8}>\n            <Alert variant=\"dark\">\n              <Alert.Heading>ORDER ID {order._id} </Alert.Heading>\n              <hr />\n              <Alert.Heading>SHIPPING</Alert.Heading>\n              <p>\n                <ListGroup>\n                  <ListGroup.Item variant=\"warning\">\n                    Name&nbsp;:-{order.address.fullName}\n                  </ListGroup.Item>\n                  <ListGroup.Item variant=\"secondary\">\n                    Phone number&nbsp;:-{order.address.phoneNumber}\n                  </ListGroup.Item>\n                  <ListGroup.Item variant=\"warning\">\n                    Country&nbsp;:-{order.address.country}\n                  </ListGroup.Item>\n                  {order.address.streetAddress !== \"\" ? (\n                    <ListGroup.Item variant=\"secondary\">\n                      Street Address&nbsp;:-{order.address.streetAddress}\n                    </ListGroup.Item>\n                  ) : null}\n                  {order.address.city !== \"\" ? (\n                    <ListGroup.Item variant=\"warning\">\n                      City&nbsp;:-{order.address.city}\n                    </ListGroup.Item>\n                  ) : null}\n                  {order.address.state !== \"\" ? (\n                    <ListGroup.Item variant=\"secondary\">\n                      State&nbsp;:-{order.address.state}\n                    </ListGroup.Item>\n                  ) : null}\n                  {order.address.zipCode !== \"\" ? (\n                    <ListGroup.Item variant=\"warning\">\n                      Zip Code&nbsp;:-{order.address.zipCode}\n                    </ListGroup.Item>\n                  ) : null}\n                </ListGroup>\n              </p>\n              <br />\n              {order.deliveryStatus === \"Delivered\" ? (\n                <Alert variant=\"success\">\n                  DELIVERY STATUS:&nbsp;{order.deliveryStatus}\n                </Alert>\n              ) : (\n                <Alert variant=\"danger\">\n                  DELIVERY STATUS: &nbsp;{order.deliveryStatus}\n                </Alert>\n              )}\n              <hr />\n              <Alert.Heading>PAYMENT METHOD</Alert.Heading>\n              <p className=\"mb-0\">\n                <ListGroup>\n                  <ListGroup.Item variant=\"light\">\n                    {order.paymentMethod}\n                  </ListGroup.Item>\n                </ListGroup>\n              </p>\n              <br />\n              {order.paidStatus === false ? (\n                <Alert variant=\"danger\">PAID STATUS:&nbsp;UNPAID</Alert>\n              ) : (\n                <Alert variant=\"success\">PAID STATUS: &nbsp;PAID</Alert>\n              )}{\" \"}\n              <hr />\n              <Alert.Heading>ORDER ITEMS</Alert.Heading>\n              {order.orderStatus === \"Placed\" ? (\n                <Alert variant=\"success\">\n                  ORDER STATUS:&nbsp;{order.orderStatus}\n                </Alert>\n              ) : (\n                <Alert variant=\"danger\">\n                  ORDER STATUS: &nbsp;{order.orderStatus}\n                </Alert>\n              )}\n              {/* render items */}\n              {renderItems}\n            </Alert>\n          </Col>\n          <Col sm={4}>\n            {\" \"}\n            <Alert variant=\"success\">\n              <Alert.Heading>ORDER SUMMARY</Alert.Heading>\n              <ListGroup>\n                <ListGroup.Item\n                  variant=\"warning\"\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  <h1>Items</h1>\n                  <p>{order.products.length}</p>\n                </ListGroup.Item>\n                <ListGroup.Item\n                  variant=\"secondary\"\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  <h1>Shipping Cost</h1>\n                  <p>NRs.{order.shippingCost}</p>\n                </ListGroup.Item>\n                <ListGroup.Item\n                  variant=\"warning\"\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  <h1>Tax</h1>\n                  <p>NRs.{order.tax}</p>\n                </ListGroup.Item>\n                <hr />\n                <ListGroup.Item\n                  variant=\"secondary\"\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  <h1>Sub Total</h1>\n                  <p>NRs.{order.totalPrice}</p>\n                </ListGroup.Item>\n              </ListGroup>\n              <hr />\n              {order.orderStatus === \"Cancelled\" ? (\n                <Button\n                  variant=\"btn btn-danger\"\n                  onClick={() => PlaceOrder(order._id)}\n                >\n                  Place Order\n                </Button>\n              ) :  (\n                <Button\n                  variant=\"btn btn-success\"\n                  onClick={() => CancelOrder(order._id)}\n                >\n                  Cancel Order\n                </Button>\n              )}\n\n              {/* <p className=\"mb-0\">\n                Whenever you need to, be sure to use margin utilities to keep\n                things nice and tidy.\n              </p> */}\n            </Alert>\n          </Col>\n        </Row>\n      </Container>\n    );\n  } else {\n    return (\n      <Container>\n        <br />\n        <Spinner animation=\"border\" variant=\"primary\" />\n        <Spinner animation=\"border\" variant=\"secondary\" />\n        <Spinner animation=\"border\" variant=\"success\" />\n        <Spinner animation=\"border\" variant=\"danger\" />\n        <Spinner animation=\"border\" variant=\"warning\" />\n        <Spinner animation=\"border\" variant=\"info\" />\n        <Spinner animation=\"border\" variant=\"light\" />\n        <Spinner animation=\"border\" variant=\"dark\" />\n        <Spinner animation=\"grow\" variant=\"primary\" />\n        <Spinner animation=\"grow\" variant=\"secondary\" />\n        <Spinner animation=\"grow\" variant=\"success\" />\n        <Spinner animation=\"grow\" variant=\"danger\" />\n        <Spinner animation=\"grow\" variant=\"warning\" />\n        <Spinner animation=\"grow\" variant=\"info\" />\n        <Spinner animation=\"grow\" variant=\"light\" />\n        <Spinner animation=\"grow\" variant=\"dark\" />\n        <Button variant=\"primary\" disabled>\n          <Spinner\n            as=\"span\"\n            animation=\"grow\"\n            size=\"sm\"\n            role=\"status\"\n            aria-hidden=\"true\"\n          />\n          Go to orders\n        </Button>\n      </Container>\n    );\n 