    const renderItems = products.map((product) => {\n      const perProductTotal = product.product.price * product.quantity;\n      var str = product.product.photo;\n      var url = str.replace(\"/home/sushil/2021/E-Commerce/api/uploads/\", \"\");\n      // console.log(url);\n      var source = \"http://localhost:4000/uploads/\" + url;\n\n      return (\n        <Fragment>\n          <div className=\"row border-top border-bottom\">\n            <div className=\"row main align-items-center\">\n              <div className=\"col-2\">\n                <img className=\"img-fluid\" src={source} alt=\"\" />\n              </div>\n              <div className=\"col\">\n                <div className=\"row text-muted\">\n                  {product.product.categoryID.name}\n                </div>\n                <div className=\"row\">{product.product.title}</div>\n              </div>\n              <div className=\"col\">\n                {\" \"}\n                <Fragment>\n                  <a href=\"#\" className=\"border\">\n                    {product.quantity}\n                  </a>\n                  &nbsp;&nbsp;\n                </Fragment>\n              </div>\n              <div className=\"col\">\n                NRS.&nbsp;{product.product.price}\n                <span className=\"close\"></span>\n              </div>\n              <div className=\"col\">\n                Total.&nbsp;{perProductTotal}\n                <span className=\"close\"></span>\n              </div>\n            </div>\n          </div>\n        </Fragment>\n      );\n    });\n    return (\n      <Container>\n        <br />\n        <Row>\n          <Col sm={8}>\n            <Alert variant=\"dark\">\n              <Alert.Heading>ORDER ID {order._id} </Alert.Heading>\n              <hr />\n              <Alert.Heading>SHIPPING</Alert.Heading>\n              <p>\n                <ListGroup>\n                  <ListGroup.Item variant=\"warning\">\n                    Name&nbsp;:-{order.address.fullName}\n                  </ListGroup.Item>\n                  <ListGroup.Item variant=\"secondary\">\n                    Phone number&nbsp;:-{order.address.phoneNumber}\n                  </ListGroup.Item>\n                  <ListGroup.Item variant=\"warning\">\n                    Country&nbsp;:-{order.address.country}\n                  </ListGroup.Item>\n                  {order.address.streetAddress !== \"\" ? (\n                    <ListGroup.Item variant=\"secondary\">\n                      Street Address&nbsp;:-{order.address.streetAddress}\n                    </ListGroup.Item>\n                  ) : null}\n                  {order.address.city !== \"\" ? (\n                    <ListGroup.Item variant=\"warning\">\n                      City&nbsp;:-{order.address.city}\n                    </ListGroup.Item>\n                  ) : null}\n                  {order.address.state !== \"\" ? (\n                    <ListGroup.Item variant=\"secondary\">\n                      State&nbsp;:-{order.address.state}\n                    </ListGroup.Item>\n                  ) : null}\n                  {order.address.zipCode !== \"\" ? (\n                    <ListGroup.Item variant=\"warning\">\n                      Zip Code&nbsp;:-{order.address.zipCode}\n                    </ListGroup.Item>\n                  ) : null}\n                </ListGroup>\n              </p>\n              <br />\n              {order.deliveryStatus === \"Delivered\" ? (\n                <Alert variant=\"success\">\n                  DELIVERY STATUS:&nbsp;{order.deliveryStatus}\n                </Alert>\n              ) : (\n                <Alert variant=\"danger\">\n                  DELIVERY STATUS: &nbsp;{order.deliveryStatus}\n                </Alert>\n              )}\n              <hr />\n              <Alert.Heading>PAYMENT METHOD</Alert.Heading>\n              <p className=\"mb-0\">\n                <ListGroup>\n                  <ListGroup.Item variant=\"light\">\n                    {order.paymentMethod}\n                  </ListGroup.Item>\n                </ListGroup>\n              </p>\n              <br />\n              {order.paidStatus === false ? (\n                <Alert variant=\"danger\">PAID STATUS:&nbsp;UNPAID</Alert>\n              ) : (\n                <Alert variant=\"success\">PAID STATUS: &nbsp;PAID</Alert>\n              )}{\" \"}\n              <hr />\n              <Alert.Heading>ORDER ITEMS</Alert.Heading>\n              {order.orderStatus === \"Placed\" ? (\n                <Alert variant=\"success\">\n                  ORDER STATUS:&nbsp;{order.orderStatus}\n                </Alert>\n              ) : (\n                <Alert variant=\"danger\">\n                  ORDER STATUS: &nbsp;{order.orderStatus}\n                </Alert>\n              )}\n              {/* render items */}\n              {renderItems}\n            </Alert>\n          </Col>\n          <Col sm={4}>\n            {\" \"}\n            <Alert variant=\"success\">\n              <Alert.Heading>ORDER SUMMARY</Alert.Heading>\n              <ListGroup>\n                <ListGroup.Item\n                  variant=\"warning\"\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  <h1>Items</h1>\n                  <p>{order.products.length}</p>\n                </ListGroup.Item>\n                <ListGroup.Item\n                  variant=\"secondary\"\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  <h1>Shipping Cost</h1>\n                  <p>NRs.{order.shippingCost}</p>\n                </ListGroup.Item>\n                <ListGroup.Item\n                  variant=\"warning\"\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  <h1>Tax</h1>\n                  <p>NRs.{order.tax}</p>\n                </ListGroup.Item>\n                <hr />\n                <ListGroup.Item\n                  variant=\"secondary\"\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  <h1>Sub Total</h1>\n                  <p>NRs.{order.totalPrice}</p>\n                </ListGroup.Item>\n              </ListGroup>\n              <hr />\n              {order.orderStatus === \"Cancelled\" ? (\n                <Button\n                  variant=\"btn btn-danger\"\n                  onClick={() => PlaceOrder(order._id)}\n                >\n                  Place Order\n                </Button>\n              ) :  (\n                <Button\n                  variant=\"btn btn-success\"\n                  onClick={() => CancelOrder(order._id)}\n                >\n                  Cancel Order\n                </Button>\n              )}\n\n              {/* <p className=\"mb-0\">\n                Whenever you need to, be sure to use margin utilities to keep\n                things nice and tidy.\n              </p> */}\n            </Alert>\n          </Col>\n        </Row>\n      </Container>\n    );\n  } else {\n    return (\n      <Container>\n        <br />\n        <Spinner animation=\"border\" variant=\"primary\" />\n        <Spinner animation=\"border\" variant=\"secondary\" />\n        <Spinner animation=\"border\" variant=\"success\" />\n        <Spinner animation=\"border\" variant=\"danger\" />\n        <Spinner animation=\"border\" variant=\"warning\" />\n        <Spinner animation=\"border\" variant=\"info\" />\n        <Spinner animation=\"border\" variant=\"light\" />\n        <Spinner animation=\"border\" variant=\"dark\" />\n        <Spinner animation=\"grow\" variant=\"primary\" />\n        <Spinner animation=\"grow\" variant=\"secondary\" />\n        <Spinner animation=\"grow\" variant=\"success\" />\n        <Spinner animation=\"grow\" variant=\"danger\" />\n        <Spinner animation=\"grow\" variant=\"warning\" />\n        <Spinner animation=\"grow\" variant=\"info\" />\n        <Spinner animation=\"grow\" variant=\"light\" />\n        <Spinner animation=\"grow\" variant=\"dark\" />\n        <Button variant=\"primary\" disabled>\n          <Spinner\n            as=\"span\"\n            animation=\"grow\"\n            size=\"sm\"\n            role=\"status\"\n            aria-hidden=\"true\"\n          />\n          Go to orders\n        </Button>\n      </Container>\n    );\n  }\n};\n\nexport default withRouter(OrderDetails);\n","/home/sushil/2021/E-Commerce/ecommerce/src/reducers/index.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/components/layouts/Spinner.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/actions/auth.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/actions/category.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/keys.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/actions/products.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/actions/alert.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/actions/order.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/components/checkout/CheckOutForm.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/actions/cart.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/reducers/auth.js",["205","206"],"import {\n  LOGIN_FAIL,\n  LOGIN_SUCCESS,\n  REGISTER_FAIL,\n  REGISTER_SUCCESS,\n  LOGOUT,\n  GOOGLE_LOGIN_SUCCESS\n} from \"../actions/types\";\n\nconst initialState = {\n  token: localStorage.getItem(\"token\"),\n  // isAuthenticated:null,\n  loading: true,\n};\n\nexport default function (state = initialState, action) {\n  const { type, payload } = action;\n  switch (type) {\n    case REGISTER_SUCCESS:\n      return state;\n      return { ...state, token: null, loading: false };\n    case LOGIN_SUCCESS:\n    case GOOGLE_LOGIN_SUCCESS:\n      // console.log(payload.token);\n      localStorage.setItem(\"token\", payload.token);\n      // console.log(localStorage.getItem(\"token\"));\n      return { ...state, ...payload, loading: false };\n\n    case REGISTER_FAIL:\n    case LOGIN_FAIL:\n      localStorage.removeItem(\"token\");\n      return { ...state, token: null, loading: false };\n    case LOGOUT:\n      localStorage.removeItem(\"token\");\n      return { ...state, token: null, loading: false };\n    default:\n      return state;\n  }\n}\n","/home/sushil/2021/E-Commerce/ecommerce/src/reducers/category.js",["207"],"import { CATEGORIES } from \"../actions/types\";\n\nconst initialState = {\n  categories: [],\n  loading: true,\n};\n\nexport default function (state = initialState, action) {\n  const { type, payload } = action;\n  switch (type) {\n    case CATEGORIES:\n      return { ...state, ...payload, loading: false };\n\n    default:\n      return state;\n  }\n}\n","/home/sushil/2021/E-Commerce/ecommerce/src/reducers/alert.js",["208"],"const { SET_ALERT, REMOVE_ALERT } = require(\"../actions/types\");\n\nconst inititalState = [];\n\nexport default function (state = inititalState, action) {\n  const { type, payload } = action;\n  switch (type) {\n    case SET_ALERT:\n      return [...state, payload];\n    case REMOVE_ALERT:\n      return state.filter((alert) => alert.id !== payload);\n    default:\n      return state;\n  }\n}\n","/home/sushil/2021/E-Commerce/ecommerce/src/reducers/customerProduct.js",["209"],"import { PRODUCTS, PRODUCT, PRODUCT_FAIL } from \"../actions/types\";\n\nconst initialState = {\n  products: [],\n  product: {},\n  loading: true,\n  error: {},\n};\n\nexport default function (state = initialState, action) {\n  const { type, payload } = action;\n  switch (type) {\n    case PRODUCTS:\n      return { ...state, ...payload, loading: false };\n\n    case PRODUCT:\n      return { ...state, product: payload, loading: false };\n    case PRODUCT_FAIL:\n      return { ...state, error: payload, loading: false };\n    default:\n      return state;\n  }\n}\n","/home/sushil/2021/E-Commerce/ecommerce/src/reducers/order.js",["210"],"import { GET_ORDERS,GET_ORDER, PLACE_ORDER,PLACE_ORDER_FAIL, PLACE_ORDER_BY_ID,CANCEL_ORDER } from \"../actions/types\";\n\nconst initialState = {\n    orders:{},\n    loading:false,\n    order:{}\n};\n\n\nexport default function (state = initialState, action) {\n  const { type, payload } = action;\n  switch (type) {\n    case PLACE_ORDER:\n        return {...state,order:payload.newOrder,loading:false}\n    case PLACE_ORDER_FAIL:\n        return state\n    case GET_ORDERS:\n        return {...state,orders:payload.orders,loading:false}\n    case GET_ORDER:\n    case CANCEL_ORDER:\n        return {...state,order:payload.order,loading:false}\n    case PLACE_ORDER_BY_ID:\n        return {...state,order:payload.order,loading:false}\n    default:\n      return state;\n  }\n}","/home/sushil/2021/E-Commerce/ecommerce/src/reducers/cart.js",["211"],"import {\n  ADD_TO_CART,\n  REMOVE_FROM_CART,\n  GET_CART_ITEMS,\n} from \"../actions/types\";\n\nconst initialState = {\n  // loading:true\n  products: [],\n  subTotal: 0,\n  user: \"\",\n  loading: true,\n};\n\nexport default function (state = initialState, action) {\n  const { type, payload } = action;\n  // console.log(payload);\n  // const {subTotal}=payload\n  // console.log(subTotal);\n  switch (type) {\n    case ADD_TO_CART:\n    case REMOVE_FROM_CART:\n      return {\n        ...state,\n        products: payload.carts.products,\n        subTotal: payload.carts.subTotal,\n        user: payload.carts.user,\n        loading: false,\n      };\n    case GET_CART_ITEMS:\n      return {\n        ...state,\n        products: payload.carts.products,\n        subTotal: payload.carts.subTotal,\n        user: payload.carts.user,\n        loading:false,\n      };\n    default:\n      return state;\n  }\n}\n","/home/sushil/2021/E-Commerce/ecommerce/src/actions/types.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/axios/axios.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/components/admin/Dashboard.js",["212","213","214"],"/home/sushil/2021/E-Commerce/ecommerce/src/components/admin/Category.js",["215","216","217"],"/home/sushil/2021/E-Commerce/ecommerce/src/components/admin/Products.js",["218"],"import React, { useEffect ,useState} from \"react\";\nimport { Table, Container, Alert, Button } from \"react-bootstrap\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { adminProducts } from \"../../actions/adminAction\";\nimport AddProductModal from \"./AddProduct\";\nconst AdminProducts = () => {\n  const [show, setShow] = useState(false);\n  const handleClose = () => setShow(false);\n  const handleShow = () => {\n    // setId(null)\n    setShow(true);\n  };\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(adminProducts());\n  }, [dispatch]);\n\n  const { products } = useSelector((state) => state.adminProduct);\n  const renderProducts = products.map((product) => {\n    var str = product.photo;\n    var url = str.replace(\"uploads/\", \"\");\n    // var url = str.replace(\"/home/sushil/2021/E-Commerce/api/uploads/\", \"\");\n    // console.log(url);\n    var source = \"http://localhost:4000/uploads/\" + url;\n    return (\n      <tr key={product._id}>\n        <td>{product._id}</td>\n        {!product.categoryID.name ? (\n          <td></td>\n        ) : (\n          <td>{product.categoryID.name}</td>\n        )}\n        <td>{product.title}</td>\n        <td>\n          <img\n            alt=\"ecommerce\"\n            src={product.photo}\n            className=\"object-cover object-center\"\n          />\n        </td>\n        <td>Rs.{product.price}</td>\n        <td>\n          <i class=\"fas fa-edit\" style={{ color: \"blue\" }}></i>\n          &nbsp;&nbsp;&nbsp;&nbsp;\n          <i class=\"fas fa-trash\" style={{ color: \"red\" }}></i>\n        </td>\n      </tr>\n    );\n  });\n\n  return (\n    <Container>\n      <AddProductModal\n        show={show}\n        setShow={setShow}\n        handleClose={handleClose}\n        handleShow={handleShow}\n        // category={category}\n        // id={id}\n      />\n      <Alert.Heading style={{ width: \"20%\" }}>\n        <Button variant=\"danger\" onClick={handleShow}>\n          <i class=\"fas fa-plus-circle\" >&nbsp;&nbsp;Add Product</i>\n        </Button>\n      </Alert.Heading>\n      <Table striped bordered hover variant=\"warning\">\n        <thead>\n          <tr>\n            <th>Product Id</th>\n            <th>Category Name</th>\n            <th>Product Name</th>\n            <th>Product Photo</th>\n            <th>Product Price</th>\n            <th>Action</th>\n          </tr>\n        </thead>\n        <tbody>{renderProducts}</tbody>\n      </Table>\n    </Container>\n  );\n};\n\nexport default AdminProducts;\n","/home/sushil/2021/E-Commerce/ecommerce/src/components/admin/AddCategory.js",["219"],"import React, { useEffect, useState } from \"react\";\nimport {Modal} from \"react-bootstrap\";\n\nimport {useDispatch, useSelector} from \"react-redux\"\nimport { AddCategory,getCategory,editCategory} from \"../../actions/adminAction\";\n\nconst AddCategoryModal=({ show, setShow, handleClose, handleShow,id})=>{\n  // console.log(id);\n    const {category}=useSelector(state=>state.adminCategory)\n    const [formData,setFormData]=useState({\n      name:\"\"\n    })\n    const dispatch=useDispatch();\n    const addCategory=async()=>{\n       dispatch(AddCategory(formData));\n       handleClose()\n    }\n     const EditCategory=async(id)=>{\n       dispatch(editCategory(formData,id));\n       handleClose()\n    }\n    useEffect(()=>{\n      if(id){\n        dispatch(getCategory(id))\n      }\n    // setFormData({name:category.name})\n    },[id])\n    useEffect(()=>{\n      setFormData({name:category.name})\n    },[category])\n\n\n    const {name}=formData;\n    const onChange=(e)=>setFormData({...formData,[e.target.name]:e.target.value})    \n    // setFormData({name:category.name});\n    return(\n        <Modal\n        size=\"md\"\n        show={show}\n        onHide={handleClose}\n        backdrop=\"static\"\n        keyboard={false}\n      >\n        <Modal.Header closeButton>\n          <Modal.Title>Category</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n             {/* <div className=\"grid md:grid-cols-3 md:gap-2\"> */}\n                <input\n                  type=\"text\"\n                  name=\"name\"\n                  className=\"border rounded h-10 w-full focus:outline-none focus:border-green-200 px-2 mt-2 text-sm\"\n                  placeholder=\"Enter category name\"\n                  value={name}\n                  onChange={(e) => onChange(e)}\n                />\n              {/* </div>{\" \"} */}\n              \n              <div className=\"flex justify-between items-center pt-2\">\n                {\" \"}\n                <button\n                  type=\"button\"\n                  onClick={handleClose}\n                  className=\"h-12 w-24 text-blue-500 text-xs font-medium\"\n                >\n                  Cancel\n                </button>\n                <button\n                  type=\"button\"\n                    onClick={!id ? addCategory : (()=>EditCategory(id))}\n                  //   className=\"btn btn-danger\"\n                  className=\"h-12 w-48 rounded font-medium text-xs bg-blue-500 text-white\"\n                >\n                  Save\n                  {/* Continue to Shipping */}\n                </button>{\" \"}\n              </div>\n        </Modal.Body>\n        </Modal>\n    )\n} \n\nexport default AddCategoryModal;","/home/sushil/2021/E-Commerce/ecommerce/src/actions/adminAction.js",[],"/home/sushil/2021/E-Commerce/ecommerce/src/reducers/adminCategory.js",["220"],"/home/sushil/2021/E-Commerce/ecommerce/src/reducers/adminProduct.js",["221"],"/home/sushil/2021/E-Commerce/ecommerce/src/components/admin/AddProduct.js",["222","223","224","225","226"],{"ruleId":"227","replacedBy":"228"},{"ruleId":"229","replacedBy":"230"},{"ruleId":"227","replacedBy":"231"},{"ruleId":"229","replacedBy":"