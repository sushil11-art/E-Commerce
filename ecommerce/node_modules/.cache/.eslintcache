tory));\n  };\n\n  // open checkout form Modal....\n\n  const [show, setShow] = useState(false);\n  const handleClose = () => setShow(false);\n  const handleShow = () => setShow(true);\n\n  if (products.length > 0 && !loading) {\n    const renderProducts = products\n      .slice(pageVisted, pageVisted + productsPerPage)\n      .map((product) => {\n        // console.log(product._id.price);\n        const perProductTotal = product._id.price * product.quantity;\n        var str = product._id.photo;\n        var url = str.replace(\"/home/sushil/2021/E-Commerce/api/uploads/\", \"\");\n        // console.log(url);\n        var source = \"http://localhost:4000/uploads/\" + url;\n        return (\n          <Fragment key={product._id._id}>\n            <div className=\"row border-top border-bottom\">\n              <div className=\"row main align-items-center\">\n                <div className=\"col-2\">\n                  <img className=\"img-fluid\" src={source} alt=\"\" />\n                </div>\n                <div className=\"col\">\n                  {product._id.categoryID.name != null ? (\n                    <div className=\"row text-muted\">\n                      {product._id.categoryID.name}\n                    </div>\n                  ) : null}\n\n                  <div className=\"row\">{product._id.title}</div>\n                </div>\n                <div className=\"col\">\n                  {\" \"}\n                  <Fragment>\n                    <button onClick={() => RemoveProduct(product._id._id)}>\n                      -\n                    </button>{\" \"}\n                    &nbsp;&nbsp;\n                    <a className=\"border\">{product.quantity}</a>&nbsp;&nbsp;\n                    <button onClick={() => AddProduct(product._id._id)}>\n                      +\n                    </button>{\" \"}\n                  </Fragment>\n                </div>\n                <div className=\"col\">\n                  NRS.&nbsp;{product._id.price}\n                  <span className=\"close\"></span>\n                </div>\n                <div className=\"col\">\n                  Total.&nbsp;{perProductTotal}\n                  <span className=\"close\"></span>\n                </div>\n              </div>\n            </div>\n          </Fragment>\n        );\n      });\n\n    return (\n      <Fragment>\n        <CheckoutForm\n          show={show}\n          setShow={setShow}\n          handleClose={handleClose}\n          handleShow={handleShow}\n        />\n        <br />\n        <div className=\"card\">\n          <div className=\"row\">\n            <div className=\"col-md-8 cart\">\n              <div className=\"title\">\n                <div className=\"row\">\n                  <div className=\"col\">\n                    <h4>\n                      <b>Shopping Cart</b>\n                    </h4>\n                  </div>\n                  <div className=\"col align-self-center text-right text-muted\">\n                    {products.length} items\n                  </div>\n                </div>\n              </div>\n              {renderProducts}\n              <br />\n              <div\n                style={{\n                  display: \"block\",\n                  marginLeft: \"auto\",\n                  marginRight: \"auto\",\n                }}\n              >\n                <ReactPaginate\n                  containerClassName={\"paginationBttns\"}\n                  previousLinkClassName={\"previousBttns\"}\n                  nextLinkClassName={\"nextBttns\"}\n                  disabledClassName={\"paginationDisabled\"}\n                  activeClassName={\"paginationActive\"}\n                  previousLabel={\"Previous\"}\n                  nextLabel={\"Next\"}\n                  pageCount={pageCount}\n                  onPageChange={changePage}\n                  breakLabel={\"...\"}\n                  marginPagesDisplayed={2}\n                  pageRangeDisplayed={5}\n                />\n              </div>\n\n              <div className=\"back-to-shop\">\n                <Link to=\"/products\">\n                  <i className=\"fas fa-arrow-left\"></i>\n                </Link>\n                <span className=\"text-muted\">\n                  <Link to=\"/products\">Back to shop</Link>\n                </span>\n              </div>\n            </div>\n            <div className=\"col-md-4 summary\">\n              <div>\n                <h5>\n                  <b>Summary</b>\n                </h5>\n              </div>\n              <hr />\n              <div className=\"row\">\n                <div className=\"col\" style={{ paddingLeft: \"0\" }}>\n                  ITEMS {products.length}\n                </div>\n                <div className=\"col text-right\">NRS.{subTotal}</div>\n              </div>\n              <br />\n              <div className=\"row\">\n                <div className=\"col\" style={{ paddingLeft: \"0\" }}>\n                  SHIPPING COST\n                </div>\n                <div className=\"col text-right\">NRS.0</div>\n              </div>\n              <br />\n              <div className=\"row\">\n                <div className=\"col\" style={{ paddingLeft: \"0\" }}>\n                  TAX\n                </div>\n                <div className=\"col text-right\">NRS.0</div>\n              </div>\n              <div className=\"row price\">\n                <div className=\"col\">TOTAL PRICE</div>\n                <div className=\"col text-right\">NRS.{subTotal}</div>\n              </div>{\" \"}\n              <button onClick={handleShow} className=\"btn\">\n                CHECKOUT\n              </button>\n            </div>\n          </div>\n        </div>\n      </Fragment>\n    );\n  } else {\n    return (\n      <Fragment>\n        <br />\n        <div className=\"container-fluid mt-100\">\n          <div className=\"row\">\n            <div className=\"col-md-12\">\n              <div className=\"card\">\n                <div className=\"card-header\">\n                  <h5>Cart</h5>\n                </div>\n                <div className=\"card-body cart\">\n                  <div className=\"col-sm-12 empty-cart-cls text-center\">\n                    {\" \"}\n                    <img\n                      style={{\n                        display: \"block\",\n                        marginLeft: \"auto\",\n                        marginRight: \"auto\",\n                      }}\n                      src=\"https://i.imgur.com/dCdflKN.png\"\n                      width=\"130\"\n                      height=\"130\"\n                      className=\"img-fluid mb-4 mr-3\"\n                      alt=\"\"\n                    />\n                    <h3>\n                      <strong>Your Cart is Empty</strong>\n                    </h3>\n                    <h4>Add something to make me happy :)</h4>{\" \"}\n                    <Link\n                      to=\"/products\"\n                      className=\"btn btn-primary cart-btn-transform m-3\"\n                      data-abc=\"true\"\n                    >\n                      Continue shopping\n                    </Link>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </Fragment>\n    );\n  }\n};\n\nexport default withRouter(Cart);\n","/home/sushil/2021/E-Commerce/ecommerce/src/components/Product/ProductDetails.js",["192","193","194"],"import React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { productDetails } from \"../../actions/products\";\nimport { Link, useHistory, withRouter } from \"react-router-dom\";\n// import Spinner from \"../layouts/Spinner\";\nimport { AddToCart } from \"../../actions/cart\";\n\nconst ProductDetails = (props) => {\n  // console.log(localStorage.getItem(\"token\"));\n  const productID = props.match.params.productID;\n  